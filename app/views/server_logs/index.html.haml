:javascript
  $(function() {
    $('#logs').dataTable({
      "bJQueryUI": true,
      "bPaginate": false,
      "bLengthChange": false,
      "bFilter": false,
      "bSort": false,
      "bInfo": false
    });
  });

  function uploadCompleteHandler(upload_options, event) {
    alert("Uploaded!")
  }

%h1!= "Server logs for " + link_to( "#{h enclosing_resource_name.humanize}: #{h enclosing_resource.name}", enclosing_resource_path )

%table#logs.wide
  %thead
    %tr
      %th Website
      %th Filename
      %th Log_format
      %th
      %th

  %tbody
    - @server_logs.each do |server_log|
      %tr
        %td= server_log.website.name
        %td= server_log.filename
        %td= server_log.log_format
        %td= link_to 'Show', website_server_log_path(server_log.website)
        %td= link_to 'Destroy', website_server_log_path(server_log.website), :confirm => 'Are you sure?', :method => :delete

%br

= raw s3_swf_upload_tag( |
  :onFileAdd => "s3_swf_1_object.startUploading();", |
  :onUploadComplete => "uploadCompleteHandler(upload_options,event);") |

%br
= link_to 'Back', :back

